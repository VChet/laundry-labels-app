    "dexie": "^3.2.4",
    "jwt-decode": "^4.0.0",
    "ky": "^1.1.3",
    "pinia": "^2.1.7",
    "quasar": "^2.14.1",
    "shepherd.js": "^11.2.0",
    "vue": "^3.3.9",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
