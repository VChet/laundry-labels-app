    "compressorjs": "^1.2.1",
    "dexie": "^3.2.5",
    "jwt-decode": "^4.0.0",
    "ky": "^1.2.0",
    "pinia": "^2.1.7",
    "quasar": "^2.14.5",
    "shepherd.js": "^11.2.0",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0"
