    "dexie": "^3.2.3",
    "jwt-decode": "^3.1.2",
    "ky": "^0.33.3",
    "pinia": "^2.1.3",
    "quasar": "^2.12.0",
    "shepherd.js": "^11.1.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
